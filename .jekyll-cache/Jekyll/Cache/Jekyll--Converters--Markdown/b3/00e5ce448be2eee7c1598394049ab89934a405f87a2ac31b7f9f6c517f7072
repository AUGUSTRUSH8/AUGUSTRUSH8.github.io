def file2matrix(filename):
    #æ‰“å¼€æ–‡ä»¶
    fr = open(filename)
    #è¯»å–æ–‡ä»¶æ‰€æœ‰å†…å®¹
    arrayOLines = fr.readlines()
    #å¾—åˆ°æ–‡ä»¶è¡Œæ•°
    numberOfLines = len(arrayOLines)
    #è¿”å›çš„NumPyçŸ©é˜µ,è§£æå®Œæˆçš„æ•°æ®:numberOfLinesè¡Œ,3åˆ—
    returnMat = np.zeros((numberOfLines,3))
    #è¿”å›çš„åˆ†ç±»æ ‡ç­¾å‘é‡
    classLabelVector = []
    #è¡Œçš„ç´¢å¼•å€¼
    index = 0
    for line in arrayOLines:
        #s.strip(rm)ï¼Œå½“rmç©ºæ—¶,é»˜è®¤åˆ é™¤ç©ºç™½ç¬¦(åŒ…æ‹¬â€™\nâ€™,â€™\râ€™,â€™\tâ€™,â€™ â€˜)
        line = line.strip()
        #ä½¿ç”¨s.split(str=â€â€œ,num=string,cout(str))å°†å­—ç¬¦ä¸²æ ¹æ®â€™\tâ€™åˆ†éš”ç¬¦è¿›è¡Œåˆ‡ç‰‡ã€‚
        listFromLine = line.split(â€˜\tâ€™)
        #å°†æ•°æ®å‰ä¸‰åˆ—æå–å‡ºæ¥,å­˜æ”¾åˆ°returnMatçš„NumPyçŸ©é˜µä¸­,ä¹Ÿå°±æ˜¯ç‰¹å¾çŸ©é˜µ
        returnMat[index,:] = listFromLine[0:3]
        #æ ¹æ®æ–‡æœ¬ä¸­æ ‡è®°çš„å–œæ¬¢çš„ç¨‹åº¦è¿›è¡Œåˆ†ç±»,1ä»£è¡¨ä¸å–œæ¬¢,2ä»£è¡¨é­…åŠ›ä¸€èˆ¬,3ä»£è¡¨æå…·é­…åŠ›
        if listFromLine[-1] == â€˜didntLikeâ€™:
            classLabelVector.append(1)
        elif listFromLine[-1] == â€˜smallDosesâ€™:
            classLabelVector.append(2)
        elif listFromLine[-1] == â€˜largeDosesâ€™:
            classLabelVector.append(3)
        index += 1
    return returnMat, classLabelVector</p>

